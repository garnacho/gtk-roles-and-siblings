      <separator name="first-sep"/>
      <toolitem name="Left" verb="justify-left"/>
      <toolitem name="Centre" verb="justify-center"/>
      <toolitem name="Right" verb="justify-right"/>
      <toolitem name="Fill" verb="justify-fill"/>
      <separator name="second-sep" />
    </placeholder>
  </dockitem>
</Root>

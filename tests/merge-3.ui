<!--*- xml -*-->
<Root>
  <menu>
    <submenu name="FileMenu" verb="StockFileMenuAction">
      <menuitem name="New" verb="New2Action" />
    </submenu>
    <placeholder name="TestPlaceholder">
      <submenu name="Test">
        <menuitem name="Cut" verb="CutAction" />
      </submenu>
    </placeholder>
  </menu>
  <dockitem name="toolbar1">
